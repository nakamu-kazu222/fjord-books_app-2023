<h1><%= t('views.users.index.title') %></h1>

<ul>
  <% @users.each do |user| %>
    <li>
      <%= link_to user.name, user_path(user) %>
      <% if user == current_user %>
        <%= link_to t('views.users.index.edit'), edit_user_path(user) %>
      <% end %>
    </li>
  <% end %>
</ul>

<%= paginate @users %>
